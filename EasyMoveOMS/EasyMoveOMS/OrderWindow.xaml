                        <ComboBoxItem Content="7" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="8" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="9" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="10" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="11" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="12" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="13" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="14" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="15" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="16" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="17" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="18" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="19" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="20" HorizontalAlignment="Left" Height="16"/>
