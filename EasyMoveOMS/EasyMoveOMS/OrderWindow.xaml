                    <Label></Label>
                    <Button Name="btSelectClient" Height="23" Content="Select" Width="60" IsEnabled="True"/>
                    <Label></Label>
                    <Button Name="btAddClient" Height="23" Content="Add new" Width="60" IsEnabled="True"/>
                </WrapPanel>
                <Label/>
                <WrapPanel>
                    <Label Content="Phone: " Height="25" VerticalAlignment="Top"/>
                    <TextBox Height="23" TextWrapping="Wrap" Text="" Width="110" IsEnabled="False" Foreground="Black"/>
                    <Label Content="(h)   " Height="25" VerticalAlignment="Top"/>
                    <TextBox Height="23" TextWrapping="Wrap" Text="" Width="110" IsEnabled="False" Foreground="Black"/>
                    <Label Content="(w)" Height="25" VerticalAlignment="Top"/>
                </WrapPanel>
                <Label/>
                <WrapPanel>
                    <Label Content="Email:  " Height="25" VerticalAlignment="Top"/>
                    <TextBox Height="23" TextWrapping="Wrap" Text="" Width="283" IsEnabled="False" Foreground="Black"/>
                    
                </WrapPanel>
                <Label/>
                <WrapPanel MinWidth="500"> 
                    <Label Content="Truck:  " Height="23" VerticalAlignment="Top"/>
