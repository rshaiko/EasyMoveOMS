            <Label/>
            <Label/>
            <Button Name="btSave" Click="btSave_Click" Content="Save" Width="110" Height="30"/>
            <Label/>
            <Label/>
            <Button Content="Export to..." Width="110" Height="30"/>
            <Label/>
            <Label/>
            <Button Name="btnCreateInvoice" Click="btnCreateInvoice_Click" Content="Create invoice" Width="110" Height="30"/>
