                        <ComboBoxItem Content="14 ft" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="20 ft" HorizontalAlignment="Left" Height="16"/>
                        <ComboBoxItem Content="26 ft" HorizontalAlignment="Left" Height="16"/>
